<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 定义模块可被调用的服务,调用方,在Spring加载过程中引用此配置-->
	<dubbo:reference id="paymentDubboService" interface="com.yatang.sc.payment.dubbo.service.PaymentDubboService" timeout="120000" validation="false" version="1.0.0" />
	<dubbo:reference id="refundDubboService" interface="com.yatang.sc.payment.dubbo.service.RefundDubboService" timeout="120000" validation="false" version="1.0.0" />
</beans>